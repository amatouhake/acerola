{
  "name": "acerola",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "open": "wait-on http://localhost/ && opener http://localhost/",
    "sync": "wait-on http://localhost/ && opener http://localhost:3000/ && browser-sync start -p localhost --open -w -f 'public/**/**'",
    "dev:cli": "webpack --watch --mode development",
    "dev:ser": "ts-node-dev src/server/index.ts",
    "dev": "run-p dev:* sync",
    "build:clean": "rimraf dist/* public/*",
    "build:tsc": "tsc",
    "build:pack": "webpack --mode production",
    "build:breakout": "cpx %USERPROFILE%/breakout/** public/Breakout",
    "build": "run-s build:*",
    "start": "node .",
    "run:noopen": "run-s build start",
    "run": "run-p run:noopen open"
  },
